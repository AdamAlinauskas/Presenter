<script src="/scripts/signalr/signalr-client-1.0.0-alpha2-final.js"></script>
<script>

    let connection = new signalR.HubConnection('/chat');

    connection.on('send', data => {
        console.log(data);
    });

    connection.start()
            .then(() => connection.invoke('send', 'Hello'));
</script>